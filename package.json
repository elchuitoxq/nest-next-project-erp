{
  "name": "temp_init",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:setup": "npm run build -w @repo/db && npm run db:setup -w @repo/db",
    "db:setup:test": "npm run build -w @repo/db && npm run db:setup:test -w @repo/db && npm run seed:transactions -w apps/api",
    "db:studio": "npm run db:studio -w @repo/db"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "turbo": "^2.8.1",
    "typescript": "5.7.3"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@11.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "react-to-print": "^3.2.0"
  }
}
